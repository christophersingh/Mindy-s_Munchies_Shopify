      }

      a.flex-prev{
        left: 10px;
        &:before{
          content: "\f104";
        }

        &:hover:before{
          
        }
      }
      a.flex-next{
        right: 10px;
        &:before{
          content: "\f105";
        }

        &:hover:before{
          
        }
      }       
