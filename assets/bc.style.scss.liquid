      }
      &:first-child,
      &:last-child {
        > a,
        > span {
          @include border-radius-circle;
        }
      }
      &.active {        
        > a,
        > span {                    
          position: relative;
          background: $button-btn1;
          border-color: $button-btn1-border;
          color: $button-btn1-txt;
          &.prev, &.next {
            color: #202020 !important;
          }
        }
      }
    }
  }
