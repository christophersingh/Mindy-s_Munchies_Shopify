#home-slideshow{  
  .swiper-slide {
    margin: 0px auto; 
    text-align: center;
    overflow: hidden;  
    height: 700px;
    background: center center; 
    background-repeat: no-repeat;
    background-size: cover;    
    > img {
      width: 100%;
      display: none;
    }
    > .container {
      position: relative;      
      margin: 0px auto;
      height: 100%;
    }
  }
  .swiper-content {   
    @include ab(20%, n, n, n);        
    text-align: center;
    padding: 0;        
    max-width: 380px; 
    display: inline-block;  
    margin: 0px auto;    
    z-index: 3; 
    @include CSStransition(all, 0.5s, ease, 0s);        
    .container-box {      
      max-width: 380px;
      background: $white;
      padding: 60px 45px;
      text-align: center;      
      position: relative;
      &:before {
        content: '';
        display: block;
        border: 1px solid $button-btn1-border;
        @include ab(10px, 10px, 10px, 10px);
      }
      > div {      
        background: transparent;      
