  	padding-top: 62px;  	
    text-align: right;
  	.cart-contact-wrapper {  	 
      float: right; 
      margin: 0 0 0 30px;
  	  .header-phone {
        float: left;
  	  	display: inline-block;
  	  	line-height: 30px;
  	  	padding: 10px 0;
  	  	color: #ed2790;
  	  	.fa {
  	  	  color: #ed2790;
  	  	  margin: 0 5px 0 0;
  	  	}
  	  }
  	}
  }
