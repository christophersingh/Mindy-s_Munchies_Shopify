            &.f-user {
              &:before {
                content: "\e693";
              }
            }
            &.f-clock {
              &:before {
                content: "\e66b";
              }
            }
            &.f-comment {
              &:before {
                content: "\e668";
              }
            }
          }
          &:first-child {
            padding-left: 0;
            &:before {
              content: none;
            }
          }         
        }       
      }
      .post-des {
        padding: 0 0 25px;
        line-height: 24px;
        a {
          &:hover {
            text-decoration: underline;
          }
        }
      }
      .buttons-wrapper {
        .link-to-post {
        display: inline-block;
        color: #a3a2a2;
        width: 24px;
        height: 24px;
        text-align: center;
        line-height: 24px;
        background: $button-btn1;
        color: $button-btn1-txt;        
        @include border-radius-circle;
        &:hover {
          background: $button-hover-btn1;
          color: $button-hover-btn1-txt;
        }
      }
      }
    }
    &:nth-child(2n+1)  {
      clear: both;
    }
    &:hover {
      
    }
  } 
}

