.product-wrapper{
  position: relative;
  padding: 0px;
  margin: 0 0 30px;
  text-align: center;
  border: 1px solid #ed2790;    
  overflow: hidden;
  @include CSStransition(all, 0.2s, ease, 0s);
  .product-head{
    .product-image{
      position: relative; 
      margin: 0 auto;
      padding: 0;         
      .switch {         
        img {             
          margin: 0px auto;
          display:inline-block;
          vertical-align: middle;          
        }
        a > span {
          img {
            @include opacity(0);
            @include ab(0%, 0, 0, 0);            
            @include CSStransition(all, 0.2s, linear, 0s);
            z-index: 0;              
            display:inline-block;
            vertical-align: middle;            
            margin: 0px auto;
          }
        }
        &:hover {               
        .featured-image {
          @include opacity(0);
        }
        a > span {
          img {
            @include opacity(1);
            z-index: 1;           
          }
        }
      }
    }
    .featured-img{
        overflow: hidden;
        position: relative;        
        text-align: center;        
        a {
          display: block;
          &:after {
            content: '';
            width: 100%;
            height: 100%;
            background: rgba(204, 204, 204, 0.7);
            z-index: 1;
            @include ab(0, 0, n, n);
            @include opacity(0);
            @include CSStransition(all, 0.3s, ease, 0s);
          }
        }
        img{            
          max-width: 100%;
          margin: 0px auto;                    
          display:inline-block;
          vertical-align: middle;          
        }
      }     
    }

    .wrapper-countdown{
      @include ab(n,1px,6px,1px);
      @include CSStransition(all, 0.35s, ease-in, 0s);
      @include opacity(0.5);      
      z-index: 2;
      text-align: center;      
      > div{
        border: 0;
        background: none;        
        color: #fff;
        font-size: $body-font-size;
        text-align: center;

        > span{
          font-weight: 400;
          display: inline-block;
          margin: 0 2px;
          padding: 5px 8px;
          font-size: 12px;
          background: $body-label-deal-background;
          color: $body-label-deal-color;          
        }
      }
    }   
    .product-button {       
      padding: 0;
      width: 100%;
      z-index: 3;              
      max-height: 48px;    
      @include ab(n, 0, 0, n);
      > div {
        margin: 0 2px 0 0;
        display: inline-block;
      }
      .add-to-wishlist {
        @include border-radius-reset;
        @include translate(0, 20px);
        @include opacity(0);
        @include transition(color 0.5s ease-out 0.001s, background 0.5s ease-out 0.001s, visibility 0.5s ease-out 0.35s, opacity 0.5s ease-out 0.35s, transform 0.5s ease-out 0.55s);
        
      }
      .add-cart-button {            
      width: 48px;
      height: 48px;
      @include translate(0, 20px);      
      @include opacity(0);
      @include transition(color 0.5s ease-out 0.001s, background 0.5s ease-out 0.001s, visibility 0.5s ease-out 0.35s, opacity 0.5s ease-out 0.35s, transform 0.5s ease-out 0.35s);     
      .add-to-cart,
      .select-options {       
        display: block;
        text-align: center;
        width: 48px;
        height: 48px;
        line-height: 48px;               
      }
    }
    .quick-shop-button {
      @include translate(0, 20px);      
      @include opacity(0);
      @include transition(color 0.5s ease-out 0.001s, background 0.5s ease-out 0.001s, visibility 0.5s ease-out 0.35s, opacity 0.5s ease-out 0.35s, transform 0.5s ease-out 0.45s);     
    }
    }
  }

  .product-review{        
    margin: 0;
    color: $body-general-color;    
    .spr-badge-caption{
      display: none;
    }
    .spr-badge-starrating {
      float: none;
    }
  }
  .product-content {
    padding: 30px;
    position: relative;
    overflow: hidden;
    text-align: center;
    .pc-inner{
      display: block;
      width: 100%;
      float: left;
    }
    .product-description {
      display: none;
    }    
    .product-name{      
      margin: 0 0 10px;
      line-height: 1.3;      
      color: $color-title;                 
      font-size: $font-medium;
      font-family: $font-product-title;
      /*white-space: nowrap;
      text-overflow: ellipsis;
      overflow: hidden;*/
      a {
        color: #ed2790;                 
        font-size: $font-medium;
        font-family: $font-product-title;
      }

      &:hover a{        
        color: #ed2790;
      }
    }    
    .product-descrip{
      display: none;
    }
    .product-price, 
    .price {    
      padding: 0;
      line-height: 1.0;
      .price-compare{        
        display: inline;        

      }
    }
  }
  &:hover {
    border-color: $button-btn1-border;
    .product-head,
    .product-head:hover {     
      .wrapper-countdown {        
        @include opacity(1);       
      }      
      .product-image {
        .featured-img {
          a {
            &:after {
              @include opacity(1);
            }
          }
        }
      }
      .product-button {
        &.fix {
          bottom: 42px;
        }
        .quick-shop-button,
        .add-cart-button,
        .add-to-wishlist {
          @include translate(0, 0px);
          @include opacity(1);
        }
      }
    }
  }
}
