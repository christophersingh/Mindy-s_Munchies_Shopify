/*site loading*/
@keyframes "loading"{
  from{left: 0;}
  to{left: 100%;}
}
@-moz-keyframes loading {
  from {
    left: 0;
  }
  to {
    left: 100%;
  }
}
@-webkit-keyframes "loading" {
  from {
    left: 0;
  }
  to {
    left: 100%;
  }
}
@-ms-keyframes "loading" {
  from {
    left: 0;
  }
  to {
    left: 100%;
  }
}
@-o-keyframes "loading" {
  from {
    left: 0;
  }
  to {
    left: 100%;
  }
}

.load-complete {
  position: fixed;
  background: #fff;
  width: 100%;
  height: 100%;
  left: 0;
  right: 0;
  top: 2px;
  bottom: 0;
  z-index: 100001;
  .load-position {
    position: absolute;
    top: 50%;
    left: 0;
    z-index: 999;
    right: 0;
    margin-top: -100px;
    h6 {
      text-align: center;
      color: #000;
      font-size: 12px;
      font-weight: 400;
      font-style: italic;
    }
  }
  .logo {
    color: #4C4C4C;
    text-align: center;
    display: block;
    margin-bottom: 20px;
    font-family: 'mission_scriptregular';
    font-size: 50px;
  }
  .loading-page {
    position: absolute;
    width: 100%;
    height: 1px;
    margin: 20px auto;
    left: 0;
    right: 0;
    .loading-line {
      position: absolute;
      background: $border-light;
      width: 100%;
      height: 2px;
    }
    .loading-break {
      position: absolute;
      background: $button-btn1;
      width: 15px;
      height: 2px;
      &.loading-dot-1 {
        -webkit-animation: loading 2s infinite;
        -moz-animation: loading 2s infinite;
        -ms-animation: loading 2s infinite;
        -o-animation: loading 2s infinite;
        animation: loading 2s infinite;
      }

      &.loading-dot-2 {
        -webkit-animation: loading 2s 0.5s infinite;
        -moz-animation: loading 2s 0.5s infinite;
        -ms-animation: loading 2s 0.5s infinite;
        -o-animation: loading 2s 0.5s infinite;
        animation: loading 2s 0.5s infinite;
      }
      &.loading-dot-3 {
        -webkit-animation: loading 2s 1s infinite;
        -moz-animation: loading 2s 1s infinite;
        -ms-animation: loading 2s 1s infinite;
        -o-animation: loading 2s 1s infinite;
        animation: loading 2s 1s infinite;
      }
    }
  }
}
/* header ----------------*/

.header-content{
  position: relative;         
  background: $header-background;
  padding: 0;  
  margin: 0;  
  .top-bar {        
    padding: 13px 0;    
    background: $header-topbar-background;
  } 
  .searchbox {
    margin: 0;
    position: relative;
    display: inline-block;    
    .search-icon {
      @include ab(0, n, n, 0px);  
      z-index: 3;
      width: 50px;
      height: 50px;
      padding: 0;
      margin: 0;
      border: none;      
      background: none;      
      &:before {        
        content: "\f002";
        font: 14px/50px Fontawesome;        
        display: block;
        width: 100%;
        height: 100%;
        text-align: center;        
        color: #ed2790;      
      }
      &:hover {
        &:before {
          color: #ed2790;
        }
      }
    }
    .navbar-form {        
      margin: 0;  
      padding: 0;          
      display: block;
      height: 50px;
      border: 2px solid $body-border-color;
      background: $white; 
    }
    .form-control {     
      width: 100%;     
      height: 46px;
      padding: 14px 60px 14px 20px;          
      border: none;
      background: none;
      line-height: 18px;       
      float: left;             
      text-transform: none;      
      @include box-shadow(inset 0 0 0 #fff);         
      &:hover,
      &:focus {
        @include box-shadow(0 0 0 #fff);    
        background: none;    
      }
    }
    
    #result-ajax-search {
      text-align: left;
      position: absolute;
      top: 100%;      
      width: 100%;      
      padding: 0px; 
      margin: 0;       
      display: none;
      z-index: 12;  
      border: 2px solid $body-border-color;
      background: $body-background-color;
      @include box-shadow(0 0px 0px rgba(0, 0, 0, 0.3));    
      ul {
        margin: 0;
        padding: 0;
        max-height: 490px;
        overflow: auto ; 
        background: $white;   
        width: 100%;
        li {
          display: block;
          overflow: hidden;
          padding: 10px 15px;
          .search-item-img {
            float: left;
            margin: 0px 15px 0 0;
            border: 1px solid $body-border-color;
            max-width: 65px;            
          }
          .search-item-title {
            display: block;
            overflow: hidden;  
            line-height: 1.8;            
            color:#191919;            
            margin: 0;
            font-size: $body-font-size;
            .marked {
              color: $link-hover-color;
            } 
            &:hover {
              color: $link-hover-color;
            }           
          }
          .product-price, .price {
            font-size: $body-font-size;
            color: #ed2790;
          }      
          .price-compare {
            font-size: $body-font-size;
            margin: 0 10px 0 0;
            text-decoration: line-through;
            color: $color-title;
          }    
          .price-sale {
            font-size: $body-font-size;
            color: $color-title;
          }
          p {
            margin: 0;                            
          }
          &:hover {
            background: #fafafa;
          }
        }
      }
    }
    &.s-loading {    
      .search-icon {
        &:before {
          content: "\f110"; 
          -webkit-animation: fa-spin 2s infinite linear;
          -moz-animation: fa-spin 2s infinite linear;
          -ms-animation: fa-spin 2s infinite linear;          
          animation: fa-spin 2s infinite linear;
          font-size: 16px;
        }
      }
    }
  }  
  .header-other {    
    text-align: right;
    position: static;
    padding: 0;
    margin: 20px 0 0 0;
  }
  .header-nav{
    border-top: 1px solid $body-border-color;
    background: $bg-main-nav;
    > .container{
      position: relative;
    }
    /*stick menu*/
    &.header-fixed {    
      position: fixed;
      z-index: 1030; /*fixed z-index higher than zoomContainer*/
      left: 0;
      right: 0;
      top: 0; 
      @include box-shadow(0 1px 10px 0 rgba(0, 0, 0, 0.2));      
      border: none;
      background: $white; 
      .header-logo {
        margin: 3px 0;
        img {
          max-height: 54px;
        }
      }      
      .header-other {
        margin: 0;
      }
      .horizontal-menu {
        border: none;
        .navigation .navbar .navbar-collapse .main-nav { 
          > li {   
            > a {
              padding: 17px 0;
              color: #ed2790;
            }     
            &:last-child {
              .dropdown-menu {
              left: auto !important;
              right: 0px !important;
              }
            }           
            &.dropdown {
              &:hover,
              &.hover {
                > .dropdown-menu {
                  margin-top: 0px !important;
                }
              }
            }
            &.logo-sticky {
              display: inline-block;
              padding-right: 44px;
              > a {
                padding: 5px 0;
                img {
                  max-height: 50px;
                }
              }
            }
            &.active,
            &:hover {
              > a {
                color: #ed2790;
              }
            }            
          }         
        }
      }
      #widget-social {
        padding: 14px 0;
        li a {
          color: #ed2790;
          &:hover {
            color: #ed2790;
          }
        }
      }         
    }         
  }
  .header-social {
    @include ab(0, n, n, 15px);
    z-index: 1;
  }
