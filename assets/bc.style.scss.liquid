    margin-bottom: 0;
    border-radius: 0;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;

    .flex-direction-nav{
      a{
        position: absolute; 
        top: 45%; 
        z-index:4; 
        display: block; 
        height: 40px; 
        width: 40px;
        @include border-radius(0, 0 , 0, 0); 
        border: none;
        text-align: center;         
