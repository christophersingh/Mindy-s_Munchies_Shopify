      span{
        &:before {
          content: "\f105";
        }
        &:hover,
        &:focus {
          background: $border-light;
        }
      }      
    }    
  }

  &:hover {   
    .owl-controls .owl-nav > div{
      @include opacity(1);
    }
  }
